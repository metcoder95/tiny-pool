{
  "name": "@metcoder95/tiny-pool",
  "version": "4.0.0",
  "description": "Extremely simplistic and tiny AsyncPool for limiting concurrency of async operations",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "tap --cov test/**/*.test.js && npm run test:ts && npm run lint",
    "test:ci": "tap --cov test/**/*.test.js && npm run test:ts && npm run lint:ci",
    "test:only": "tap --only",
    "test:unit": "tap test/*.test.js",
    "test:ts": "tsd",
    "lint": "standard | snazzy",
    "lint:ci": "standard",
    "release": "npx standard-version"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "async-pool",
    "pool",
    "tiny",
    "tiny-async-pool",
    "tiny-pool"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metcoder95/tiny-pool.git"
  },
  "readme": "https://github.com/metcoder95/tiny-pool/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/metcoder95/tiny-pool/issues"
  },
  "author": "metcoder95 <me@metcoder.dev>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20",
    "husky": "^9.0.11",
    "semver": "^7.3.7",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^16.3.0",
    "tsd": "^0.30.0",
    "typescript": "^5.0"
  },
  "dependencies": {},
  "tsd": {
    "directory": "test"
  },
  "tap": {
    "check-coverage": false
  },
  "standard": {
    "ignore": [
      "*.d.ts",
      "*.test-d.ts"
    ]
  }
}
